<!DOCTYPE html>
<html>
  <head>
    <title><%="Eddy2 - " + yield_for(:title,'Untitled')%></title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div id="main">
      <div id="head">
        <h1><%=yield_for(:title,"Untitled")+" - "%>Eddy 2.0</h1>
      </div>

      <%=render :partial=>'layouts/navbar'%>

      <div id="content">
        <p id="notice"><%= notice %></p>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
