<!DOCTYPE html>
<html>

  <head>
    <title>
      <%="Eddy2 - " + yield_for(:title,'Untitled')%>
    </title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag :defaults %>
    <%= csrf_meta_tag %>
  </head>

  <body>
    <div id="main">
      <%=render :partial=>'layouts/navbar'%>
      <div id="content">
        <%=content_tag(:div,yield(:sidebar),:class=>"sidebar")unless content_for(:sidebar).blank?%>
        <div class="inner_box inner_content<%=" with_sidebar" unless content_for(:sidebar).blank? %>" >
          <%= content_tag(:p,notice,:class=>"notice")unless notice.nil? %>
          <%= yield %>
        </div>
      </div>
    </div>
  </body>

</html>
