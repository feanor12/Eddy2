%li
  =form_for @user_session, :as=>:user_session , :url => user_session_path do |f| 
    =f.label :email 
    %br
    =f.text_field :email 
    %br
    =f.label :password 
    %br 
    =f.password_field :password 
    %br
    =link_to "Forgot password", new_password_reset_path
    %br
    =f.submit "Login"
